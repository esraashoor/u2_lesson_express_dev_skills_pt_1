<%- include("../partials/header") %>
        <h1>Express To-Do</h1>
        <h3>skills as of <%= time %></h3>
        <ul>
          <a href="/skills/new">Add To-Do</a>
          <% skills.forEach( skill => { %>
          
            <li>
              <a href="/todos/<%= skill.id %>/edit">Edit</a>
              <form action="/todos/<%= skill.id %>?_method=DELETE"
                class="delete-form" method="POST">
                <button type="submit">X</button>
              </form>
              <a href="/todos/<%= skill.id %>"><%= skill.skill %></a>
        <% }); %>
        </ul>
        <%- include("../partials/footer") %>
  